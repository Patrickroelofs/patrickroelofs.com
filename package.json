{
  "name": "patrickroelofs.com",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation ultracite check",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "migrate:reset": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh"
  },
  "dependencies": {
    "@fontsource-variable/national-park": "^5.2.6",
    "@payloadcms/db-mongodb": "~3.72.0",
    "@payloadcms/live-preview-react": "~3.72.0",
    "@payloadcms/next": "~3.72.0",
    "@payloadcms/richtext-lexical": "~3.72.0",
    "@payloadcms/storage-s3": "~3.72.0",
    "@payloadcms/ui": "~3.72.0",
    "@phosphor-icons/react": "^2.1.10",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tailwindcss/postcss": "^4.1.18",
    "cross-env": "^10.1.0",
    "gsap": "^3.14.2",
    "next": "15.5.9",
    "payload": "~3.72.0",
    "plaiceholder": "^3.0.0",
    "postcss": "^8.5.6",
    "react": "~19.2.3",
    "react-dom": "~19.2.3",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "lint-staged": "^16.2.7",
    "typescript": "^5.9.3",
    "ultracite": "7.0.11"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@tailwindcss/oxide",
      "esbuild",
      "sharp"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "pnpm dlx ultracite fix"
    ]
  }
}
